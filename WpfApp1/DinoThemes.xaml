<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="ButtonStyles" TargetType="Button">
        <Setter Property="Background" Value="Green"></Setter>
    </Style>
    <Style x:Key="GridStyles" TargetType="Grid">
        <Setter Property="Background" Value="#ffffff"></Setter>
    </Style>
    <Style x:Key="TextBlockStyles" TargetType="TextBlock">
        <Setter Property="Foreground" Value="Black"></Setter>
    </Style>
    <Style x:Key="BorderStyles" TargetType="Border">
        <Setter Property="Background" Value="#cafad8"></Setter>
    </Style>
</ResourceDictionary>