<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="ButtonStyles" TargetType="Button">
        <Setter Property="Background" Value="#fe5503"></Setter>
    </Style>
    <Style x:Key="GridStyles" TargetType="Grid">
        <Setter Property="Background" Value="#fdd3bf"></Setter>
    </Style>
    <Style x:Key="TextBlockStyles" TargetType="TextBlock">
        <Setter Property="Background" Value="Black"></Setter>
    </Style>
    <Style x:Key="BorderStyles" TargetType="Border">
        <Setter Property="Background" Value="#ff9562"></Setter>
    </Style>
</ResourceDictionary>