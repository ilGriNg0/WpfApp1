<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="ButtonStyles" TargetType="Button">
        <Setter Property="Background" Value="#bd93f9"/>
    </Style>
    <Style x:Key="GridStyles" TargetType="Grid">
        <Setter Property="Background" Value="#282a36"/>
    </Style>
    <Style x:Key="TextBlockStyles" TargetType="TextBlock">
        <Setter Property="Foreground"  Value="#f8f8f2"/>
    </Style>
    <Style x:Key="BorderStyles" TargetType="Border">
        <Setter Property="Background" Value="#6272a4"/>
    </Style>
 </ResourceDictionary>